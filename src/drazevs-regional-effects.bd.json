{
  "version": "0.1.0",
  "id": "drazevs-regional-effects",
  "title": "Drazevs Regional Effects",
  "description": "This is a module for the FoundryVTT system designed to extend the Regional Effects functionality introduced in Foundry Core V12",
  "authors": [
    {
      "name": "Jayson Mendoza (Drazev)",
      "url": "https://www.jkmconsulting.ca",
      "discord": "drazev"
    }
  ],
  "projectUrl": "https://github.com/JaysonMendoza/foundryvtt-Drazevs-Regional-Effects",
  "entryPoints": {
    "main" : "init.mjs",
    "lang" : "static/lang/**",
    "compendia" : {
      "path": "./packs/**",
      "type":"Macro",
      "label" : {
        "macros-gm" : "DRE Macros (GM)",
        "macros-player" : "DRE Macros (Player)"
      },
      "folder" :  {
        "label" : "Drazevs Regional Effects",
        "color" : "#FF0000"
      }
    }
  },
  "profile": {
    "dev":{
      "dest":"../",
      "compress":false,
      "sourcemaps": true,
      "clean": true,
      "hmr": true
    }
  },
  "dependencies": {
    "core" : [12,12,12]
  },
  "packs": [
    {
      "name": "macros-gm",
      "label": "DRE Macros (GM)",
      "module": "drazevs-regional-effects",
      "path": "./packs/macro-gm.db",
      "type": "Macro",
      "ownership": {
        "PLAYER": "NONE",
        "ASSISTANT": "OWNER"
      }
    },
    {
      "name": "macros-player",
      "label": "DRE Macros (Player)",
      "module": "drazevs-regional-effects",
      "path": "./packs/macro-player.db",
      "type": "Macro"
    }
  ],
  "flags" : {
    "readme": "https://github.com/JaysonMendoza/foundryvtt-Drazevs-Regional-Effects/blob/c8b53d5650ed00203ae84b0d4a556c254d168ade/README.md",
    "bugs": "https://github.com/JaysonMendoza/foundryvtt-Drazevs-Regional-Effects/issues",
    "changelog": "https://github.com/JaysonMendoza/foundryvtt-Drazevs-Regional-Effects/releases/latest/"
  }

}